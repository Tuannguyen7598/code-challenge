{
	"name": "crud-server",
	"version": "1.0.0",
	"description": "Backend server with ExpressJS and TypeScript using Clean Architecture",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
		"test": "jest",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"typeorm": "typeorm-ts-node-commonjs",
		"migrate:generate": "npm run typeorm migration:generate -- -d src/common/database/DataSource.ts",
		"migrate:run": "npm run typeorm migration:run -- -d src/common/database/DataSource.ts",
		"migrate:revert": "npm run typeorm migration:revert -- -d src/common/database/DataSource.ts",
		"migrate:show": "npm run typeorm migration:show -- -d src/common/database/DataSource.ts",
		"seed": "node scripts/seed.js"
	},
	"keywords": [
		"express",
		"typescript",
		"clean-architecture",
		"crud"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"bcrypt": "^6.0.0",
		"better-sqlite3": "^11.10.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"express-validator": "^7.0.1",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"reflect-metadata": "^0.2.2",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.7",
		"typeorm": "^0.3.25",
		"uuid": "^9.0.1",
		"winston": "^3.11.0"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.13",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.8",
		"@types/node": "^20.10.0",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.13.0",
		"@typescript-eslint/parser": "^6.13.0",
		"eslint": "^8.54.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^5.3.2"
	},
	"engines": {
		"node": ">=22.15.0"
	}
}
